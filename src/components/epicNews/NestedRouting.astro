---
import NestedRoutingReactModule from '../react/NestedRouting/ParentChildRoutes.jsx'
interface Props {
  parentFilename?: string;
  childFilename?: string;
  childUrl?: string;
}

const { parentFilename = "root.tsx", childFilename = "_index.tsx", childUrl = "" } = Astro.props;
---

<NestedRoutingReactModule
  client:visible={true}
  parentFilename={parentFilename}
  childFilename={childFilename}
  childUrl={childUrl} />
